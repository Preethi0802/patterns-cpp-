int main(){                                                                                                       1
    int n;
    cin>>n;
    int i=1,j;
    while(i<=n){
        j=1;
        while(j<=n-i+1){
            cout<<n-i+1;
            j++;
        }
        cout<<endl;
        i++;
    }
}


